{
    "version": 2,
    "builds": [
        {
            "src": "backend/app/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@now/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/app/app.py"
        }
    ]
}