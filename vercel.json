{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/api/todo/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}