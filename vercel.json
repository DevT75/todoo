{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "backend/app/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/app/app.py"
        }
    ]
}